<script type="text/javascript">
    var board = JXG.JSXGraph.initBoard('box', { boundingbox: [-10, 10, 10, -10], axis: true });

    var a = board.create('slider', [[-1, -8], [-8, -8], [10, 1, -10]], { snapWidth: 1 });
    var b = board.create('slider', [[-1, -7], [-8, -7], [10, 0, -10]], { snapWidth: 1 });
    var c = board.create('slider', [[-1, -6], [-8, -6], [10, 0, -10]], { snapWidth: 1 });

    var f = function (x) { return a.Value() * x * x + b.Value() * x + c.Value(); };

    var curve = board.create('functiongraph', [f, -10, 10], { strokecolor: 'blue', strokewidth: 2 });

    var labela = board.create('text', [0, -8, function () { return 'a = ' + a.Value().toFixed(2); }]);
    var labelb = board.create('text', [0, -7, function () { return 'b = ' + b.Value().toFixed(2); }]);
    var labelc = board.create('text', [0, -6, function () { return 'c = ' + c.Value().toFixed(2); }]);







    var board = JXG.JSXGraph.initBoard('box1', { boundingbox: [-10, 10, 10, -10], axis: true });

    var freq = board.create('slider', [[-8, 8], [-8, 7], [1, 1, 10]], { snapWidth: 1 });

    var f = function (x) { return Math.sin(freq.Value() * x); };

    var curve = board.create('functiongraph', [f, -10, 10], { strokecolor: 'blue', strokewidth: 2 });

    var labelfreq = board.create('text', [-7, 8, function () { return 'Frequency: ' + freq.Value().toFixed(0); }]);







    var board = JXG.JSXGraph.initBoard('root', { boundingbox: [-10, 10, 10, -10], axis: true });

    var aa = board.create('slider', [[-1, -8], [-8, -8], [10, 0, -10]], { snapWidth: 1 });
    var bb = board.create('slider', [[-1, -7], [-8, -7], [10, 0, -10]], { snapWidth: 1 });

    var f = function (x) { return Math.sqrt(x - aa.Value()) + bb.Value(); };

    var curve = board.create('functiongraph', [f, -10, 10], { strokecolor: 'blue', strokewidth: 2 });

    var labela = board.create('text', [0, -8, function () { return 'a = ' + aa.Value().toFixed(2); }]);
    var labelb = board.create('text', [0, -7, function () { return 'b = ' + bb.Value().toFixed(2); }]);
</script>
